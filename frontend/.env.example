# Frontend environment variables
# Copy to .env and set the backend URL

# Backend base URL used by the API client
VITE_API_BASE_URL=https://your-backend.onrender.com

# EVM contract addresses
# Set factory address per network. In development, bsc testnet is recommended.
VITE_FACTORY_ADDRESS=0xYourBSCMainnetFactory
VITE_FACTORY_ADDRESS_TESTNET=0xYourBSCTestnetFactory